{
  "expo": {
    "name": "Lunation",
    "slug": "lunation",
    "version": "1.0.0",
    "scheme": "lunation",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3b5f9eb2-5f25-4034-81cb-abe7f83345cb"
      }
    },
    "owner": "shughen85",
    "ios": {
      "bundleIdentifier": "com.shughen85.astroialunar",
      "buildNumber": "1",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.shughen85.astroialunar",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
