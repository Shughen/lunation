  "description": "Application de r√©volutions lunaires mensuelles",
  "scripts": {
    "dev:api": "cd apps/api && python -m uvicorn main:app --reload --port 8000",
    "dev:mobile": "cd apps/mobile && npx expo start",
    "db:migrate": "cd apps/api && alembic upgrade head",
    "db:create": "cd apps/api && alembic revision --autogenerate -m",
    "test:api": "cd apps/api && pytest tests/ -v",
    "test:mobile": "cd apps/mobile && npm test",
    "install:api": "cd apps/api && pip install -r requirements.txt",
    "install:mobile": "cd apps/mobile && npm install"
  },
  "repository": {
    "type": "git",
